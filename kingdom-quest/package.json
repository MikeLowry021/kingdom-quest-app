{
  "name": "kingdom-quest",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,yaml,yml}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,yaml,yml}\"",
    "type-check": "tsc --noEmit",
    "analyze": "ANALYZE=true next build",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage --reporter=verbose --reporter=json-summary --reporter=html",
    "test:ui": "vitest --ui",
    "test:unit": "vitest --run tests/unit",
    "test:theology": "vitest --run --reporter=verbose tests/unit/theology",
    "test:safety": "vitest --run --reporter=verbose tests/unit/safety",
    "test:content": "vitest --run --reporter=verbose tests/unit/content",
    "test:age-tier": "vitest --run --reporter=verbose tests/unit/age-tier",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:a11y": "playwright test --grep=accessibility",
    "test:user-journey": "playwright test tests/e2e/user-journey",
    "test:auth": "playwright test tests/e2e/auth",
    "test:navigation": "playwright test tests/e2e/navigation",
    "test:accessibility": "vitest --run --reporter=verbose tests/accessibility",
    "test:all": "npm run test:unit && npm run test:e2e && npm run test:a11y",
    "test:critical": "npm run test:theology && npm run test:safety && npm run test:user-journey",
    "test:smoke": "playwright test --grep=smoke",
    "validate:scripture": "node scripts/validate-scripture.js",
    "validate:doctrine": "node scripts/validate-doctrine.js",
    "validate:content-filter": "node scripts/validate-content-filter.js",
    "validate:age-tiers": "node scripts/validate-age-tiers.js",
    "validate:env": "npx tsx scripts/validate-env.ts",
    "i18n:extract": "npx tsx scripts/extract-translations.ts",
    "i18n:check": "npx tsx scripts/check-translations.ts",
    "i18n:update": "npx tsx scripts/update-translations.ts",
    "report:coverage": "vitest --coverage --reporter=html",
    "report:a11y": "node scripts/generate-a11y-report.js",
    "report:theology": "node scripts/generate-theology-report.js"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@googlemaps/react-wrapper": "^1.2.0",
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@react-google-maps/api": "^2.20.7",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/auth-helpers-react": "^0.5.0",
    "@supabase/supabase-js": "^2.56.0",
    "autoprefixer": "^10.4.21",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.541.0",
    "next": "15.5.0",
    "next-intl": "^3.26.5",
    "postcss": "^8.5.6",
    "react": "19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "19.1.0",
    "react-hot-toast": "^2.6.0",
    "sharp": "^0.34.3",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.0",
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.48.2",
    "@tailwindcss/postcss": "^4.1.12",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "axe-core": "^4.10.2",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "happy-dom": "^15.11.6",
    "msw": "^2.6.4",
    "playwright": "^1.48.2",
    "prettier": "^3.2.5",
    "tailwindcss": "^4",
    "tailwindcss-rtl": "^0.9.0",
    "tsx": "^4.19.2",
    "typescript": "^5",
    "vercel": "^46.0.2",
    "vitest": "^2.1.8",
    "vitest-axe": "^0.1.0",
    "zod": "^3.23.8"
  }
}
