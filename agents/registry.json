  "routing_rules": {
    "content_generation": {
      "primary_flow": ["theology_guard", "story_weaver", "visual_psalmist", "audio_psalmist"],
      "validation_required": true,
      "parallel_execution": ["visual_psalmist", "audio_psalmist"],
      "fallback_strategy": "simplified_content"
    },
    "quiz_creation": {
      "primary_flow": ["theology_guard", "quiz_master", "ux_builder"],
      "validation_required": true,
      "parallel_execution": [],
      "fallback_strategy": "template_quiz"
    },
    "family_activity": {
      "primary_flow": ["theology_guard", "community_host", "ux_builder"],
      "validation_required": true,
      "parallel_execution": ["map_cartographer"],
      "fallback_strategy": "basic_activity"
    },
    "content_moderation": {
      "primary_flow": ["safety_moderator", "theology_guard"],
      "validation_required": true,
      "parallel_execution": [],
      "fallback_strategy": "human_review"
    }
  },
  "health_monitoring": {
    "check_interval_seconds": 30,
    "timeout_seconds": 5,
    "unhealthy_threshold": 3,
    "recovery_threshold": 2
  },
  "load_balancing": {
    "algorithm": "weighted_round_robin",
    "health_aware": true,
    "sticky_sessions": false
